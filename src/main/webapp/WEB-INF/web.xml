<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>com.revature.servlets.SignupServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewReimb</servlet-name>
		<servlet-class>com.revature.servlets.ViewReimbServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CreateNew</servlet-name>
		<servlet-class>com.revature.servlets.CreateNewServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateServlet</servlet-name>
		<servlet-class>com.revature.servlets.UpdateServlet</servlet-class>
	</servlet>
 
  <servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/SignUp</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewReimb</servlet-name>
		<url-pattern>/ViewReimb</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CreateNew</servlet-name>
		<url-pattern>/CreateNew</url-pattern>
	</servlet-mapping>
  	<servlet-mapping>
		<servlet-name>UpdateServlet</servlet-name>
		<url-pattern>/UpdateServlet</url-pattern>
	</servlet-mapping>
  
</web-app>
